{
  "plugins": ["transform-polyfills", "transform-object-rest-spread"],
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["last 2 versions"]
            },
            "modules": false
          }
        ]
      ],
      "plugins": [
        [
          "transform-runtime",
          { "polyfill": true, "regenerator": false, "helpers": true }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["last 2 versions"]
            }
          }
        ]
      ],
      "plugins": [
        [
          "transform-runtime",
          { "polyfill": false, "regenerator": false, "helpers": true }
        ]
      ]
    }
  }
}
